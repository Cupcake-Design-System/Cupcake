{
  "name": "@cupcake-ds/cupcake",
  "version": "2.0.4",
  "description": "Design System framework.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cupcake-Design-System/Cupcake/"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "lint": "stylelint src/scss/ --syntax scss || true",
    "build": "run-s lint && gulp prod",
    "release": "release-it",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "start": "gulp",
    "precommit": "lint-staged && npm run lint"
  },
  "browserslist": [
    "> 2%",
    "IE 10"
  ],
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "commitLimit": false,
    "package": true
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "autoprefixer": "^10.1.0",
    "browser-sync": "^2.26.13",
    "del": "^5.1.0",
    "fabricator-assemble": "^1.4.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-changed": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "lint-staged": "^10.5.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "release-it": "^14.2.2",
    "sass-true": "^6.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-strict-value": "^1.7.5",
    "stylelint-scss": "^3.18.0"
  }
}
