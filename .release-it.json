{
    "pkgFiles": ["package.json"],
    "buildCommand": "gulp build",
    "changelogCommand": "auto-changelog --template changelog-template.hbs --commit-limit false -o CHANGELOG.md",
    "dist": {
        "baseDir": null
    },
    "github": {
        "release": true,
        "releaseName": "Release %s",
        "preRelease": false,
        "draft": false,
        "tokenRef": "GITHUB_TOKEN"
    }    
}